
page {
			background-color: #f5f5f5;
}
.search{
		  -webkit-border-radius: 50rpx;
		          border-radius: 50rpx;
		  display: -webkit-box;
		  display: -webkit-flex;
		  display: flex;
}
.top-icon{
		 width: 75rpx;
		 height: 60rpx; 
		 padding: 4px 0 0 15px;
}
.search-icon{
	  		  width: 25rpx;
			  height: 40rpx;
}
.see-icon{
		  width: 40rpx;
		  height: 35rpx;
}
.list {
	  		padding: 9rpx;
}
.top-line{
	 		  background-color: #00AA00;
	 		  display: -webkit-box;
	 		  display: -webkit-flex;
	 		  display: flex;
}
.top-line-style{
		 margin: 5px;
		 padding-left:30px;
		 font-size: 17px;
		 color: #FFFFFF;
}
.fenlei-title{
		 margin: 15px 20px 0 20px;
		 padding-top: 10px;
		 font-weight:bold;
		 font-size: 18px;
		 color: #808080;
		 border-top: 1px solid #C8C7CC;
}
.fenlei-div1{
		  border: 1px ridge #C8C7CC;
		  margin: 10px 20px 0 20px;  
		  padding: 8px;
		  display: -webkit-box;
		  display: -webkit-flex;
		  display: flex;
}
.fenlei-div-demo{
		 margin: 2px 20px 2px 40px;
		 color: #808080;
}
.scroll-view_H{
		    margin-top: 12px;
			width: 100%;
		  	height: auto;
		  	overflow: hidden;
		   	white-space: nowrap;
}
.scroll-view-item_H{
		   display: inline-block;
			height: 120px;
			border: 2px solid #F1F1F1;
}
.scroll-view-item_name{
		 text-align: center;
		  color: #808080;
		  font-size: 13px;
		  font-weight: bold;
}
.fenlei-dital{
		  display: -webkit-box;
		  display: -webkit-flex;
		  display: flex;
		  margin: 15px 20px 0 20px;
		  padding-bottom: 8px;
		  font-size: 11px;
		  color: #808080;	
		  border-bottom: 2px solid #e6e6e6;
}
.fenlei-dital-word{
		 margin-right: 10px;
		 background-color: #e6e6e6;
		 -webkit-border-radius: 50rpx;
		         border-radius: 50rpx;
		 padding: 2px 5px 0 5px;
}
.fenlei-dital-number{
		 margin:0 10px 0 0;
}
.fenlei-dital-day{
		 padding: 0 0 0 92px;
		 text-align: right;
}
.content{
		 margin: 15px 20px 0 20px;
		 padding-bottom: 8px;
		 font-size: 15px;
		 color: #808080;	
		 border-bottom: 2px solid #e6e6e6;
}
.content-numbAndDay{
		padding: 2px 0 2px 15px;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
}
.content-dital-day{
		float: right;
		padding: 0 0 0 110px;
}
	
